{
  "aggregatedFlow": {

    "dynamic_templates": [
      {
        "strings": {
          "match_mapping_type": "string",
          "mapping": {
            "type": "string",
            "index": "not_analyzed"
          }
        }
      }
    ],

    "properties": {
      "agentJvm": {
        "properties": {
          "accountId": {
            "type": "string",
            "index": "not_analyzed"
          },
          "agentId": {
            "type": "string",
            "index": "not_analyzed"
          },
          "jvmId": {
            "type": "string",
            "index": "not_analyzed"
          }
        }
      },

      "time": {
        "type": "date"
      },

      "dataType": {
        "type": "string",
        "index": "not_analyzed"
      },

      "rootFlow": {
        "properties": {
          "flowId": {
            "type": "string",
            "index": "not_analyzed"
          },
          "throughputPerSec": {
            "type": "float"
          },
          "maxTime": {
            "type": "long"
          },
          "minTime": {
            "type": "long"
          },
          "averageTime": {
            "type": "long"
          },
          "threadStatistics": {
            "properties": {
              "threadId": {
                "type": "string",
                "index": "not_analyzed"
              },
              "threadState": {
                "type": "string",
                "index": "not_analyzed"
              },
              "threadCount": {
                "type": "float"
              }
            }
          }
        }
      }
    }
  }
}
