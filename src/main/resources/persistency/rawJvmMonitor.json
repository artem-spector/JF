{
  "cpu": {
    "properties": {
      "agentJvm": {
        "properties": {
          "accountId": {
            "type": "string",
            "index": "not_analyzed"
          },
          "agentId": {
            "type": "string",
            "index": "not_analyzed"
          },
          "jvmId": {
            "type": "string",
            "index": "not_analyzed"
          }
        }
      },

      "time": {
        "type": "date"
      },

      "processCpuLoad": {
        "type": "float"
      },

      "liveThreads": {
        "properties": {
          "threadId": {
            "type": "long"
          },
          "threadName": {
            "type": "string",
            "index": "not_analyzed"
          },
          "threadState": {
            "type": "string",
            "index": "not_analyzed"
          },
          "stackTrace": {
            "properties": {
              "className": {
                "type": "string",
                "index": "not_analyzed"
              },
              "methodName": {
                "type": "string",
                "index": "not_analyzed"
              },
              "fileName": {
                "type": "string",
                "index": "not_analyzed"
              },
              "lineNumber": {
                "type": "integer"
              }
            }
          }
        }
      }
    }
  }
}